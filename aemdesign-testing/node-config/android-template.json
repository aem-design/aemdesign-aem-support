{
  "capabilities":
  [
    {
      "browserName": "Chrome",
      "version":"7.1.1",
      "platformVersion":"7.1.1",
      "maxInstances": 1,
      "platformName": "android",
      "deviceName": "Pixel_2_API_25",
      "chromedriverArgs": ["--no-first-run"],
      "chromeOptions": {
        "prefs": {
          "distribution": {
            "skip_first_run_ui": true
          }
        }
      }
    }
  ],
  "configuration":
  {
    "cleanUpCycle":2000,
    "timeout":5000,
    "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
    "url":"http://APPIUM_IP:APPIUM_PORT/wd/hub",
    "port": "APPIUM_PORT",
    "hub": "http://SELENIUM_IP:SELENIUM_PORT/grid/register",
    "hubPort": "SELENIUM_PORT",
    "hubHost": "SELENIUM_IP",
    "maxSession": 1,
    "register": true,
    "registerCycle": 5000,
    "role": "node",
    "unregisterIfStillDownAfter": 5000,
    "debug": false,
    "nodeStatusCheckTimeout": 5000
  }
}