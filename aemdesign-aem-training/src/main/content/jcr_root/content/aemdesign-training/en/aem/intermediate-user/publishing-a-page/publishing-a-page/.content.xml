<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[aemdesign:content-type/page]"
        cq:template="/conf/aemdesign-training/settings/wcm/templates/twocolumn"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:lockable,mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Publishing a Page"
        sling:resourceType="aemdesign/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"/>
                <pagelist
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/lists/pagelist"
                    componentTheme="[]"
                    listFrom="children"
                    listType="list"
                    parentPage="/content/aemdesign-training/en/aem/intermediate-user/publishing-a-page"
                    printStructure="{Boolean}true"
                    tagsMatch="any"/>
                <contentblock
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentTheme="[]"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>Navigate until you can see the page you want to replicate to the publish environment in the card / list view.&lt;/li>&#xa;&lt;li>Use the &lt;b>Publish&lt;/b> page icon for your required page(s):&lt;br>&#xa;&lt;img src=&quot;/content/dam/aemdesign-training/Publishing_a_Page-Publish_Button.png&quot;>&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                        <text_780310012
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>With selection mode you can select &lt;b>Publish&lt;/b>&amp;nbsp;from the menu that appears:&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                        <image_1708799629
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/media/image"
                            fileReference="/content/dam/aemdesign-training/Publishing_a_Page-Publish_Menu.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="500"/>
                        <text_2107766595
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>Publish will replicate the page to the publish environment.&lt;/li>&#xa;&lt;/ul>&#xa;&lt;p>The page can also be published directly from the page editor:&lt;/p>&#xa;&lt;ul>&#xa;&lt;li>Select the &lt;b>Page Information&lt;/b> icon to open the menu:&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                        <image_503363948
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/media/image"
                            fileReference="/content/dam/aemdesign-training/Publishing_a_Page-Publish_Menu_from_Page_Editor.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="200"/>
                        <text_1086700777
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>Select &lt;b>Publish Page&lt;/b>.&lt;/li>&#xa;&lt;/ul>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock>
            </par>
        </article>
    </jcr:content>
</jcr:root>
