<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[aemdesign:content-type/page]"
        cq:template="/conf/aemdesign-training/settings/wcm/templates/twocolumn"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:lockable,mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Search Contents by Tags"
        sling:resourceType="aemdesign/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"/>
                <pagelist
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/lists/pagelist"
                    componentTheme="[]"
                    listFrom="children"
                    listType="list"
                    parentPage="/content/aemdesign-training/en/aem/intermediate-user/using-tags"
                    printStructure="{Boolean}true"
                    tagsMatch="any"/>
                <contentblock
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentTheme="[]"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;p>You can search contents by tags from several ways:&lt;/p>&#xa;&lt;ul>&#xa;&lt;li>Content Finder on the Page Edit mode.&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                        <image
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/media/image"
                            fileReference="/content/dam/aemdesign-training/Using_Tags-Conent_Finder.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="200"/>
                        <text_1267610963
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>Search on Site Console.&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                        <image_1699991257
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/media/image"
                            fileReference="/content/dam/aemdesign-training/Using_Tags-Search_Tags_on_Site.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="500"/>
                        <text_1933801991
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>Search on Asset Console.&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                        <image_1605837091
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/media/image"
                            fileReference="/content/dam/aemdesign-training/Using_Tags-Search_Tags_on_Asset.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="700"/>
                        <text_370658609
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>Search Component. Adding a&amp;nbsp;Search component&amp;nbsp;to a page provides a search capability which includes tags.&lt;/li>&#xa;&lt;/ul>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock>
            </par>
        </article>
    </jcr:content>
</jcr:root>
