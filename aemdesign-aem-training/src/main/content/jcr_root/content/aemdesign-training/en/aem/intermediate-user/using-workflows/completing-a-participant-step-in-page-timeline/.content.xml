<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[aemdesign:content-type/page]"
        cq:template="/conf/aemdesign-training/settings/wcm/templates/twocolumn"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:lockable,mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Completing a Participant step in Page Timeline"
        sling:resourceType="aemdesign/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"/>
                <pagelist
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/lists/pagelist"
                    componentTheme="[]"
                    listFrom="children"
                    listType="list"
                    parentPage="/content/aemdesign-training/en/aem/intermediate-user/using-workflows"
                    printStructure="{Boolean}true"
                    tagsMatch="any"/>
                <contentblock
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentTheme="[]"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>You can also use timeline to complete and advance a step:&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                        <image
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/media/image"
                            fileReference="/content/dam/aemdesign-training/Using_a_Workflow-Next_Step_in_Timeline.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="500"/>
                        <text_1211247594
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>Depending on the workflow, you can select the next step, for instance, select &lt;b>Create Version&lt;/b>:&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                        <image_558994679
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/media/image"
                            fileReference="/content/dam/aemdesign-training/Using_a_Workflow-Create_Version_in_Timeline.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="500"/>
                        <text_578781817
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>Click on &lt;b>Advance&lt;/b> to advance to the next step.&lt;/li>&#xa;&lt;li>Follow and take actions on the rest of the steps until the workflow is completed.&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                        <image_1653426910
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/media/image"
                            fileReference="/content/dam/aemdesign-training/Using_a_Workflow-Create_Version_in_Timeline.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="500"/>
                        <text_168201972
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>Click on &lt;b>Advance&lt;/b> to advance to the next step.&lt;/li>&#xa;&lt;li>Follow and take actions on the rest of the steps until the workflow is completed.&lt;/li>&#xa;&lt;/ul>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock>
            </par>
        </article>
    </jcr:content>
</jcr:root>
