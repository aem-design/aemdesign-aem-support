<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[aemdesign:content-type/page]"
        cq:template="/conf/aemdesign-training/settings/wcm/templates/twocolumn"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:lockable,mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Uploading Packages"
        sling:resourceType="aemdesign/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"/>
                <pagelist
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/lists/pagelist"
                    componentTheme="[]"
                    listFrom="children"
                    listType="list"
                    parentPage="/content/aemdesign-training/en/aem/administrator/packaging"
                    printStructure="{Boolean}true"
                    tagsMatch="any"/>
                <contentblock
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentTheme="[]"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <text_1907134826
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>Navigate to the &lt;b>Package Manager&lt;/b>. Then to the group folder into which you want the package to uploaded.&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                        <image_1586087139
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/media/image"
                            fileReference="/content/dam/aemdesign-training/Packaging-Upload_Package_Dialog.png"
                            imageOption="asIs"
                            imageRotate="0"
                            width="500"/>
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>&lt;i>&lt;b>File – &lt;/b>You can either type the file name directly, or use the &lt;b>Browse...&lt;/b> dialog to select the required package from your local file system (after selection click &lt;b>OK&lt;/b>).&lt;/i>&lt;/li>&#xa;&lt;li>&lt;i>&lt;b>Force Upload&lt;/b> – If a package with this name already exists you can click this to force upload (and overwrite the existing package).&lt;/i>&lt;/li>&#xa;&lt;/ul>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;ul>&#xa;&lt;li>Click &lt;b>OK&lt;/b> so that the new package is uploaded and listed in the Package Manager list.&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock>
            </par>
        </article>
    </jcr:content>
</jcr:root>
