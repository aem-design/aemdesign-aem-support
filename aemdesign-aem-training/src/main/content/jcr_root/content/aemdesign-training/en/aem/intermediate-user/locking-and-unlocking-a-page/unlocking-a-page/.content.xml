<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[aemdesign:content-type/page]"
        cq:template="/conf/aemdesign-training/settings/wcm/templates/twocolumn"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:lockable,mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Unlocking a Page"
        sling:resourceType="aemdesign/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"/>
                <pagelist
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/lists/pagelist"
                    componentTheme="[]"
                    listFrom="children"
                    listType="list"
                    parentPage="/content/aemdesign-training/en/aem/intermediate-user/locking-and-unlocking-a-page"
                    printStructure="{Boolean}true"
                    tagsMatch="any"/>
                <contentblock
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentTheme="[]"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;p>A locked page can be unlocked from the &lt;b>Sites&lt;/b> console:&lt;/p>&#xa;&lt;ul>&#xa;&lt;li>Use the unlock icon:&lt;br>&#xa;&lt;img src=&quot;/content/dam/aemdesign-training/Locking_Unlocking-Unlock_Button.png&quot;>&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                        <text_1378668245
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>A message will be shown to confirm that the page has been unlocked. &amp;nbsp;&lt;/li>&#xa;&lt;/ul>&#xa;&lt;p>A locked page can also be unlocked when editing the page itself:&lt;/p>&#xa;&lt;ul>&#xa;&lt;li>When you are editing a page, the fact that it is locked is shown in the toolbar (top right). Select this icon to unlock the page:&lt;br>&#xa;&lt;img src=&quot;/content/dam/aemdesign-training/Locking_Unlocking-Lock_Icon.png&quot;>&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                        <text_791873067
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>A dialog will open. Use &lt;b>Unlock Page&lt;/b> to confirm that you want to unlock the content.&lt;/li>&#xa;&lt;li>The page will be unlocked and the toolbar will be updated.&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock>
            </par>
        </article>
    </jcr:content>
</jcr:root>
