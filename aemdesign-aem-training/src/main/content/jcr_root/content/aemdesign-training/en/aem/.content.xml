<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[aemdesign:content-type/page]"
        cq:template="/conf/aemdesign-training/settings/wcm/templates/twocolumn"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:lockable,mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="AEM Training"
        sling:resourceType="aemdesign/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"/>
                <contentblock
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentTheme="[]"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;p>This is a training web site of Adobe Experience Manager (AEM) which provides the knowledge of AEM's Out-Of-The-Box (OOTB) functionalities and features allowing users to use AEM as a content management platform to author, build and publish your own web pages.&amp;nbsp;&lt;/p>&#xa;&lt;p>The training site is divided to 2 major sections:&lt;/p>&#xa;&lt;ol>&#xa;&lt;li>&lt;b>Author&lt;/b> - Separated in 3 different levels - Basic, Intermediate and Advanced that given AEM users to understand the capabilities including step-by-step walkthough on AEM's authoring functionalities.&amp;nbsp;&lt;/li>&#xa;&lt;li>&lt;b>Administrator&lt;/b> - Knowledge more on administrative level (Non-Infrastructure related) such as user group and user access administration, metadata managmeent, content replication, and etc.&lt;/li>&#xa;&lt;/ol>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;"
                            textIsRich="true">
                            <cq:annotations jcr:primaryType="nt:unstructured"/>
                        </text>
                        <image
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/media/image"
                            fileReference="/content/dam/aemdesign-training/Training-Main_Roadmap.jpg"
                            imageOption="asIs"
                            imageRotate="0"
                            variant="imageOnly"
                            width="2000"/>
                        <text_276079823
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentTheme="[]"
                            text="&lt;h1>Let's start the journey to learn how AEM can give you the new content authoring experience!&lt;/h1>&#xa;&lt;h2>&amp;nbsp;&lt;/h2>&#xa;&lt;h2>&amp;nbsp;&lt;/h2>&#xa;&lt;h2>&amp;nbsp;&lt;/h2>&#xa;&lt;h2>&amp;nbsp;&lt;/h2>&#xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock>
            </par>
        </article>
        <header
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/header"
            cancelInheritParent="true">
            <bgimage
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/media/image"
                imageRotate="0"/>
            <logo
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/media/image"
                imageRotate="0"/>
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <reference
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/content/reference"
                    path="/content/aemdesign-training/en/aem/layout/header/jcr:content/article/par/navlist"/>
            </par>
        </header>
        <banner
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/header"
            cancelInheritParent="true">
            <bgimage
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/media/image"
                imageRotate="0"/>
            <logo
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/media/image"
                imageRotate="0"/>
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <reference
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/content/reference"
                    path="/content/aemdesign-training/en/aem/admin/banner/jcr:content/article/par"/>
            </par>
        </banner>
    </jcr:content>
    <basic-user/>
    <intermediate-user/>
    <advanced-user/>
    <administrator/>
    <admin/>
</jcr:root>
