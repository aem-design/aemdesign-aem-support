                    sling:resourceType="aemdesign/components/lists/pagelist"
                    componentTheme="[]"
                    listFrom="static"
                    listType="list"
                    pages="/content/aemdesign-training/en/aem/intermediate-user/locking-and-unlocking-a-page/exercise-locking-and-unlocking-a-page"
