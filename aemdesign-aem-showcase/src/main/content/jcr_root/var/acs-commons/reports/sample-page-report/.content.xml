<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:description="A report allowing for displaying pages by template and path."
        jcr:primaryType="nt:unstructured"
        jcr:title="Page Report"
        sling:resourceType="acs-commons/components/utilities/report-builder/report-page">
        <config/>
        <parameters/>
        <columns
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys">
            <path/>
            <text/>
            <componenttree
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/reports/report-builder/columns/componenttree"
                componentattribute="jcr:title"
                exporter="design.aem.reports.PageComponentTreeReportCellCSVExporter"
                heading="Component Tree"/>
            <components
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/reports/report-builder/columns/components"
                exporter="design.aem.reports.PageComponentsReportCellCSVExporter"
                heading="Component List"/>
            <date/>
            <tags/>
            <replicationstatus/>
            <editor/>
        </columns>
    </jcr:content>
</jcr:root>
