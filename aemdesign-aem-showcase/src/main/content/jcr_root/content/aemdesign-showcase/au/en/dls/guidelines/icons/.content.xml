<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/styleguide"
        jcr:primaryType="cq:PageContent"
        jcr:title="Icons"
        sling:resourceType="aemdesign/components/template/styleguide">
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/aside"
            cancelInheritParent="false">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container"/>
            <bgimage
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/media/image"/>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page_details
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"
                    componentId="page_details"
                    editable="{Boolean}false"
                    showBreadcrumb="false"
                    showPageDate="false"
                    showParsys="false"
                    showToolbar="false"
                    textIsRich="true"/>
                <contentblock_icon_sizing
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_icon_sizing">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_icon_sizing_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_icon_sizing_heading"
                            text="&lt;h2 class=&quot;h4&quot;>Icon Sizes&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <colctrl_icon_sizes
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/colctrl"
                            componentId="colctrl_icon_sizes"
                            controlType="start"
                            layout="4;col-sm-,6,6,6,6;col-lg-,3,3,3,3"/>
                        <contentblock_icon_large
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/contentblock"
                            componentId="contentblock_icon_large">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="aemdesign/components/layout/container">
                                <contentblock_icon_large_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/embedsource"
                                    componentId="contentblock_icon_large_component"
                                    html="&lt;div class=&quot;icon--large&quot; style=&quot;padding-top: 10px;&quot;>&#xd;&#xa;&#xd;&#xa;&lt;i class=&quot;icon fas fa-search&quot;>&lt;/i>&#xd;&#xa;"/>
                            </par>
                        </contentblock_icon_large>
                        <colctrl_icon_sizes_col_break15804297570340
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/colctrl"
                            controlType="break"/>
                        <contentblock_icon_regular
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/contentblock"
                            componentId="contentblock_icon_regular">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="aemdesign/components/layout/container">
                                <contentblock_icon_regular_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/embedsource"
                                    componentId="contentblock_icon_regular_component"
                                    html="&lt;div class=&quot;icon--regular&quot; style=&quot;padding-top: 10px;&quot;>&#xd;&#xa;&#xd;&#xa;&lt;i class=&quot;icon fas fa-search&quot;>&lt;/i>&#xd;&#xa;"/>
                            </par>
                        </contentblock_icon_regular>
                        <colctrl_icon_sizes_col_break15804297570341
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/colctrl"
                            controlType="break"/>
                        <contentblock_icon_small
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/contentblock"
                            componentId="contentblock_icon_small">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="aemdesign/components/layout/container">
                                <contentblock_icon_small_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/embedsource"
                                    componentId="contentblock_icon_small_component"
                                    html="&lt;div class=&quot;icon--small&quot; style=&quot;padding-top: 10px;&quot;>&#xd;&#xa;&#xd;&#xa;&lt;i class=&quot;icon fas fa-search&quot;>&lt;/i>&#xd;&#xa;"/>
                            </par>
                        </contentblock_icon_small>
                        <colctrl_icon_sizes_col_break15804297570342
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/colctrl"
                            controlType="break"/>
                        <contentblock_icon_smaller
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/contentblock"
                            componentId="contentblock_icon_smaller">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="aemdesign/components/layout/container">
                                <contentblock_icon_smaller_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/embedsource"
                                    componentId="contentblock_icon_smaller_component"
                                    html="&lt;div class=&quot;icon--smaller&quot; style=&quot;padding-top: 10px;&quot;>&#xd;&#xa;&#xd;&#xa;&lt;i class=&quot;icon fas fa-search&quot;>&lt;/i>&#xd;&#xa;"/>
                            </par>
                        </contentblock_icon_smaller>
                        <colctrl_icon_sizes_col_end15804297405190
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/colctrl"
                            controlType="end"/>
                    </par>
                </contentblock_icon_sizing>
                <contentblock_icons_all
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_icons_all">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_icons_all_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_icons_all_heading"
                            text="&lt;h2 class=&quot;h4&quot;>Available Icons&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <contentblock_icons_all_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/embedsource"
                            componentId="contentblock_icons_all_component"
                            html="Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book. It has survived not only five centuries, but also the leap into electronic typesetting, remaining essentially unchanged. It was popularised in the 1960s with the release of Letraset sheets containing Lorem Ipsum passages, and more recently with desktop publishing software like Aldus PageMaker including versions of Lorem Ipsum."/>
                    </par>
                </contentblock_icons_all>
            </par>
        </article>
    </jcr:content>
</jcr:root>
