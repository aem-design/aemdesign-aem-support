<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/onecolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Tag Page3"
        sling:resourceType="aemdesign/components/template/base"
        subtitle="Page Sub-Title 3">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page-details
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"
                    cardIcon="[aemdesign-showcase:component-style-modifier/icon/size/sm,aemdesign-showcase:component-style-icon/social/github]"
                    cardIconShow="true"
                    componentIcon="[aemdesign-showcase:component-style-icon/social/github]"
                    componentId="page-details"
                    componentModifiers="[aemdesign-showcase:component-style-modifier/icon/size/sm]"
                    showBreadcrumb="false"
                    showParsys="false"
                    showToolbar="false"
                    textIsRich="true"/>
            </par>
        </article>
        <image
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/media/image"
            fileReference="/content/dam/aemdesign-showcase/en/components/media/image/city2.jpg"/>
    </jcr:content>
</jcr:root>
