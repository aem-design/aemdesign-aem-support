<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[aemdesign-showcase:content-type/page]"
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/onecolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Data Layer"
        sling:resourceType="aemdesign/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"
                    analyticsPageName="aemdesign-showcase:en:component:analytics:datalayer"
                    analyticsPageType="common"
                    componentId="pagedetails1"/>
                <contentblockmenu1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblockmenu"
                    componentId="contentblockmenu1"/>
                <contentblock1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock1"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="Data Layer"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            text="This template has embedded Analytics data layer component"
                            textIsRich="true"
                            variant="simple"/>
                    </par>
                </contentblock1>
            </par>
        </article>
    </jcr:content>
</jcr:root>
