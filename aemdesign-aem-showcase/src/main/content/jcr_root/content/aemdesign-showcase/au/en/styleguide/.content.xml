<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[aemdesign-showcase:content-type/page]"
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/twocolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Styleguide"
        sling:resourceType="aemdesign/components/template/base"
        sling:vanityPath="/components">
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/aside"
            cancelInheritParent="true"
            componentId="aside">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <navlist
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/lists/navlist"
                    cancelInheritParent="true"
                    componentId="componentsnavlist"/>
            </par>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <pagedetails
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details">
                    <breadcrumb
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/breadcrumb"/>
                    <toolbar
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/navbar"
                        cancelInheritParent="true"/>
                </pagedetails>
                <text
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/content/text"
                    text="&lt;p>Welcome to the Styleguide, please visit the DLS page one level down.&lt;/p>&#xa;"
                    textIsRich="true"/>
            </par>
        </article>
    </jcr:content>
    <dls/>
</jcr:root>
