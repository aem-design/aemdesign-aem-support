<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[aemdesign-showcase:content-type/page/event]"
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/onecolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Event Details 7"
        sling:resourceType="aemdesign/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <event-details
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/event-details"
                    componentId="event-details"
                    description="&lt;p>Event Description&lt;/p>&#xa;"
                    eventEndDate="{Date}2016-05-05T00:00:00.000+08:00"
                    eventLoc=""
                    eventRefLabel="Event Label"
                    eventRefLink="/content/aemdesign-showcase/au/en/component/lists/event-list"
                    eventStartDate="{Date}2016-05-01T16:19:00.000+08:00"
                    subTitleFormat="aemdesign-showcase:component-style-theme/details/event-details/format/subtitle/daterangewithlocation"
                    title="Event Title">
                    <secondaryImage
                        jcr:primaryType="nt:unstructured"
                        fileReference="/content/dam/projects/media/cover"
                        imageRotate="0"/>
                </event-details>
            </par>
        </article>
        <image
            jcr:primaryType="nt:unstructured"
            fileReference="/content/dam/aemdesign-showcase/galleries/misc/1st-photo-2.jpg"
            imageRotate="0"/>
    </jcr:content>
</jcr:root>
