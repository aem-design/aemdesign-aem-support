<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:allowedTemplates="[/conf/aemdesign-showcase/settings/wcm/templates/.*]"
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/onecolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Text"
        sling:resourceType="aemdesign/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <contentblock1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock1"
                    title="Default"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Text"
                            sling:resourceType="aemdesign/components/forms/text"
                            name="text1"
                            rows="2"
                            type="text"/>
                    </par>
                </contentblock1>
                <contentblock2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock2"
                    title="Textarea"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Textarea"
                            sling:resourceType="aemdesign/components/forms/text"
                            name="text2"
                            rows="2"
                            type="textarea"/>
                    </par>
                </contentblock2>
                <contentblock3
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock3"
                    title="Email"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Email"
                            sling:resourceType="aemdesign/components/forms/text"
                            name="text3"
                            rows="2"
                            type="email"/>
                    </par>
                </contentblock3>
                <contentblock4
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock4"
                    title="Telephone"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Telephone"
                            sling:resourceType="aemdesign/components/forms/text"
                            name="text4"
                            rows="2"
                            type="tel"/>
                    </par>
                </contentblock4>
                <contentblock5
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock5"
                    title="Date"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Date"
                            sling:resourceType="aemdesign/components/forms/text"
                            name="text5"
                            rows="2"
                            type="date"/>
                    </par>
                </contentblock5>
                <contentblock6
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock6"
                    title="Number"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <text
