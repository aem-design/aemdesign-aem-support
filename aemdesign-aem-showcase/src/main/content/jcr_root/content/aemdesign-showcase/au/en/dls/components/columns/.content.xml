<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
		  jcr:primaryType="cq:Page">
	<jcr:content
		cq:template="/conf/aemdesign-showcase/settings/wcm/templates/styleguide"
		jcr:primaryType="cq:PageContent"
		jcr:title="Columns"
		sling:resourceType="aemdesign/components/template/styleguide">
		<aside
			jcr:primaryType="nt:unstructured"
			sling:resourceType="aemdesign/components/layout/aside">
			<par
				jcr:primaryType="nt:unstructured"
				sling:resourceType="aemdesign/components/layout/container"/>
		</aside>
		<article
			jcr:primaryType="nt:unstructured"
			sling:resourceType="aemdesign/components/layout/article">
			<par
				jcr:primaryType="nt:unstructured"
				sling:resourceType="aemdesign/components/layout/container">
				<page_details
					jcr:mixinTypes="[cq:Taggable]"
					jcr:primaryType="nt:unstructured"
					sling:resourceType="aemdesign/components/details/page-details"
					componentId="page_details"
					componentModifiers="[aemdesign-showcase:component-style-modifier/background/primary/bg-yellow-pattern]"
					showBreadcrumb="false"
					showPageDate="false"
					showToolbar="false"
					textIsRich="true"/>
				<contentblock_two_columns
					jcr:primaryType="nt:unstructured"
					sling:resourceType="aemdesign/components/layout/contentblock"
					componentId="textIsRich">
					<par
						jcr:primaryType="nt:unstructured"
						sling:resourceType="aemdesign/components/layout/container">
						<text
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/content/text"
							componentId="contentblock_two_columns_title"
							text="&lt;h4>Two Columns&lt;/h4>&#xd;&#xa;&lt;p>Two columns that collapse to one on mobile.&#xd;&#xa;&lt;/p>&#xd;&#xa;"
							textIsRich="true"/>
						<colctrl
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/layout/colctrl"
							componentId="contentblock_two_columns_colctrl"
							controlType="start"
							layout="2;col-sm-,12,12;col-md-,6,6"/>
						<text_2
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/content/text"
							componentId="contentblock_two_columns_colctrl_col1_text"
							text="&lt;h2>Arts &amp;amp; Humanities&lt;/h2>&#xd;&#xa;&lt;p>We believe an understanding of language, people and culture is key to effecting world change. That's what makes aemdesign arts and humanities courses special.&lt;br />&#xd;&#xa;&lt;/p>&#xd;&#xa;"
							textIsRich="true"/>
						<col_break15573003694320
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/layout/colctrl"
							componentModifiers="[aemdesign-showcase:component-style-modifier/spacing/vertical/base]"
							controlType="break"/>
						<image
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/media/image"
							componentId="contentblock_two_columns_colctrl_col2_image"
							fileReference="/content/dam/aemdesign-showcase/en/components/media/image/city3.jpg"/>
						<col_end15573002345370
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/layout/colctrl"
							controlType="end"/>
					</par>
				</contentblock_two_columns>
			</par>
		</article>
	</jcr:content>
</jcr:root>
