<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/dls"
        jcr:primaryType="cq:PageContent"
        jcr:title="Link"
        sling:resourceType="aemdesign/components/template/dls">
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/aside">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container"/>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page_details
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"
                    componentId="page_details"
                    editable="{Boolean}false"
                    showBreadcrumb="false"
                    showPageDate="false"
                    showParsys="false"
                    showToolbar="false"
                    textIsRich="true"/>
                <link_default
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="link_default">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <link_default_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="link_default_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Default&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <link_default_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/link"
                            componentId="link_default_component"
                            label="DLS Homepage"
                            linkUrl="/content/aemdesign-showcase/au/en/dls"/>
                    </par>
                </link_default>
                <link_button_link
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="link_button_link">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <link_button_link_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="link_button_link_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Button - Link&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <link_button_link_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/link"
                            componentId="link_button_link_component"
                            componentModifiers="[aemdesign-showcase:component-style-modifier/button/link]"
                            label="DLS Homepage"
                            linkUrl="/content/aemdesign-showcase/au/en/dls"/>
                    </par>
                </link_button_link>
                <link_buttons
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="link_buttons">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <link_buttons_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="link_buttons_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Buttons&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <link_buttons_primary_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/link"
                            componentId="link_buttons_primary_component"
                            componentModifiers="[aemdesign-showcase:component-style-modifier/button/primary]"
                            label="DLS Homepage"
                            linkUrl="/content/aemdesign-showcase/au/en/dls"/>
                        <link_buttons_secondary_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/link"
                            componentId="link_buttons_secondary_component"
                            componentModifiers="[aemdesign-showcase:component-style-modifier/button/secondary]"
                            label="DLS Homepage"
                            linkUrl="/content/aemdesign-showcase/au/en/dls"/>
                    </par>
                </link_buttons>
            </par>
        </article>
    </jcr:content>
</jcr:root>
