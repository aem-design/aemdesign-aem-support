<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/dls"
        jcr:primaryType="cq:PageContent"
        jcr:title="Link"
        sling:resourceType="aemdesign/components/template/dls">
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/aside">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container"/>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page_details
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"
                    componentId="page_details"
                    editable="{Boolean}false"
                    showBreadcrumb="false"
                    showPageDate="false"
                    showParsys="false"
                    showToolbar="false"
                    textIsRich="true"/>
                <link_default
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="link_default">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <link_default_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="link_default_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Default&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <link_default_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/link"
                            componentId="link_default_component"
                            label="DLS Homepage"
                            linkUrl="/content/aemdesign-showcase/au/en/dls"/>
                    </par>
                </link_default>
                <link_button_link
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="link_button_link">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <link_button_link_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="link_button_link_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Button - Link&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <link_button_link_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/link"
                            componentId="link_button_link_component"
                            componentModifiers="[aemdesign-showcase:component-style-modifier/button/link]"
                            label="DLS Homepage"
                            linkUrl="/content/aemdesign-showcase/au/en/dls"/>
                    </par>
                </link_button_link>
                <link_buttons
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="link_buttons">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <link_buttons_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="link_buttons_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Buttons&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <link_buttons_wrapper
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/contentblock"
                            componentId="link_buttons_wrapper"
                            variant="parsys">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="aemdesign/components/layout/parsys">
                                <link_buttons_primary_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_buttons_primary_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/primary]"
                                    label="Primary"
                                    linkUrl="#"/>
                                <link_buttons_secondary_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_buttons_secondary_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/secondary]"
                                    label="Secondary"
                                    linkUrl="#"/>
                                <link_buttons_success_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_buttons_success_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/success]"
                                    label="Success"
                                    linkUrl="#"/>
                                <link_buttons_warning_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_buttons_warning_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/warning]"
                                    label="Warning"
                                    linkUrl="#"/>
                                <link_buttons_danger_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_buttons_danger_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/danger]"
                                    label="Danger"
                                    linkUrl="#"/>
                                <link_buttons_light_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_buttons_light_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/light]"
                                    label="Light"
                                    linkUrl="#"/>
                                <link_buttons_dark_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_buttons_dark_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/dark]"
                                    label="Dark"
                                    linkUrl="#"/>
                            </par>
                        </link_buttons_wrapper>
                    </par>
                </link_buttons>
                <link_buttons_outline
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="link_buttons_outline">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <link_buttons_outline_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="link_buttons_outline_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Buttons - Outline&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <link_buttons_outline_wrapper
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/contentblock"
                            componentId="link_buttons_outline_wrapper"
                            variant="parsys">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="aemdesign/components/layout/parsys">
                                <link_buttons_outline_primary_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_buttons_outline_primary_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/outline/primary]"
                                    label="Primary"
                                    linkUrl="#"/>
                                <link_buttons_outline_secondary_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_buttons_outline_secondary_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/outline/secondary]"
                                    label="Secondary"
                                    linkUrl="#"/>
                                <link_buttons_outline_success_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_buttons_outline_success_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/outline/success]"
                                    label="Success"
                                    linkUrl="#"/>
                                <link_buttons_outline_warning_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_buttons_outline_warning_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/outline/warning]"
                                    label="Warning"
                                    linkUrl="#"/>
                                <link_buttons_outline_danger_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_buttons_outline_danger_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/outline/danger]"
                                    label="Danger"
                                    linkUrl="#"/>
                                <link_buttons_outline_light_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_buttons_outline_light_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/outline/light]"
                                    label="Light"
                                    linkUrl="#"/>
                                <link_buttons_outline_dark_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_buttons_outline_dark_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/outline/dark]"
                                    label="Dark"
                                    linkUrl="#"/>
                            </par>
                        </link_buttons_outline_wrapper>
                    </par>
                </link_buttons_outline>
                <link_button_group
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    ariaRole="group"
                    componentId="link_button_group">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <link_button_group_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="link_button_group_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Button - Group&lt;/h2>&#xd;&#xa;&lt;p>A Content Block is used as the parent element with a variant of &lt;strong>parsys&lt;/strong>. This is important to ensure the structure is minimal.&lt;/p>&#xd;&#xa;"
                            textIsRich="true"/>
                        <link_button_group_wrapper
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/contentblock"
                            componentId="link_button_group_wrapper"
                            componentModifiers="[aemdesign-showcase:component-style-modifier/button/group]"
                            variant="parsys">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="aemdesign/components/layout/parsys">
                                <link_button_group_component_one
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_button_group_component_one"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/secondary]"
                                    label="Button one"
                                    linkUrl="#"/>
                                <link_button_group_component_two
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_button_group_component_two"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/secondary]"
                                    label="Button two"
                                    linkUrl="#"/>
                                <link_button_group_component_three
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_button_group_component_three"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/secondary]"
                                    label="Button three"
                                    linkUrl="#"/>
                            </par>
                        </link_button_group_wrapper>
                    </par>
                </link_button_group>
                <link_button_sizes
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="link_button_sizes">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <link_button_sizes_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="link_button_sizes_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Button - Sizes&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <link_button_sizes_small_wrapper
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/contentblock"
                            componentId="link_button_sizes_small_wrapper"
                            variant="parsys">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="aemdesign/components/layout/parsys">
                                <link_button_sizes_small_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_button_sizes_small_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/primary,aemdesign-showcase:component-style-modifier/button/size/sm]"
                                    label="Small button"
                                    linkUrl="#"/>
                                <link_button_sizes_standard_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_button_sizes_standard_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/primary]"
                                    label="Standard button"
                                    linkUrl="#"/>
                                <link_button_sizes_large_component
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/link"
                                    componentId="link_button_sizes_large_component"
                                    componentModifiers="[aemdesign-showcase:component-style-modifier/button/primary,aemdesign-showcase:component-style-modifier/button/size/lg]"
                                    label="Large button"
                                    linkUrl="#"/>
                            </par>
                        </link_button_sizes_small_wrapper>
                    </par>
                </link_button_sizes>
                <link_button_disabled
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="link_button_disabled">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <link_button_disabled_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="link_button_disabled_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Button - Disabled&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <link_button_disabled_large_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/link"
                            componentId="link_button_disabled_large_component"
                            componentModifiers="[aemdesign-showcase:component-style-modifier/button/primary,aemdesign-showcase:component-style-modifier/button/state/disabled]"
                            label="Disabled button"
                            linkUrl="#"/>
                    </par>
                </link_button_disabled>
            </par>
        </article>
    </jcr:content>
</jcr:root>
