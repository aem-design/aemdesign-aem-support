<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[aemdesign-showcase:content-type/page]"
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/onecolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="News List"
        sling:resourceType="aemdesign/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article"
            componentId="article1">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"
                    componentId="page-details"/>
                <contentblockmenu1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblockmenu"
                    componentId="contentblockmenu1"/>
                <contentblock1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock1"
                    title="Badge: Default"
                    variant="componentConfig">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <newslist
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/newslist"
                            componentId="newslist1"
                            listFrom="children"
                            parentPage="/content/aemdesign-showcase/au/en/content/news/2019/01"/>
                    </par>
                </contentblock1>
                <contentblock2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock2"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="Badge: Card with Image, Title, Description and Action with Pagination"
                    variant="componentConfig">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <newslist
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/newslist"
                            componentId="newslist2"
                            detailsBadge="cardImageTitleDescriptionAction"
                            limit="5"
                            listFrom="children"
                            pageMax="2"
                            parentPage="/content/aemdesign-showcase/au/en/content/news/2019/01"/>
                    </par>
                </contentblock2>
                <contentblock3
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock3"
                    title="Badge: Card with Image, Title, Category, Description and Action"
                    variant="componentConfig">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <newslist
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/newslist"
                            componentId="newslist3"
                            detailsBadge="cardImageTitleCategoryDescriptionAction"
                            limit="2"
                            listFrom="children"
                            parentPage="/content/aemdesign-showcase/au/en/content/news/2019/01"/>
                    </par>
                </contentblock3>
                <contentblock4
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock4"
                    title="Badge: Card with Image, Title, Subtitle, Description and Action"
                    variant="componentConfig">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <newslist
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/newslist"
                            componentId="newslist4"
                            detailsBadge="cardImageTitleSubtitleDescriptionAction"
                            limit="2"
                            listFrom="children"
                            parentPage="/content/aemdesign-showcase/au/en/content/news/2019/01"/>
                    </par>
                </contentblock4>
                <contentblock5
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock5"
                    title="Badge: Card with Image, Title, Category, Action and Date"
                    variant="componentConfig">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <newslist
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/newslist"
                            componentId="newslist5"
                            detailsBadge="cardImageTitleCategoryActionDate"
                            limit="2"
                            listFrom="children"
                            parentPage="/content/aemdesign-showcase/au/en/content/news/2019/01"/>
                    </par>
                </contentblock5>
            </par>
        </article>
    </jcr:content>
</jcr:root>
