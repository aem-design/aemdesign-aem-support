                                        sling:resourceType="aemdesign/components/content/text"
                                        componentId="text_in_toolbar"
                                        text="Text in Toolbar"
                                        textIsRich="true"/>
                                </par>
                            </toolbar>
                            <bgimage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="aemdesign/components/media/image"
