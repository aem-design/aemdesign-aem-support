                            sling:resourceType="aemdesign/components/details/page-details"
                            componentId="page-details6"
                            description="Custom Description"
                            hideDescription="true"
                            hideTitle="true"
                            showBreadcrumb="false"
                            showPageDate="false"
                            showParsys="false"
                            showToolbar="false"
                            textIsRich="true"
                            title="Page Title"
                            variant="default">
