<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[aemdesign-showcase:content-type/page]"
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/twocolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Templates"
        sling:resourceType="aemdesign/components/template/base"
        sling:vanityPath="/templates">
        <image
            jcr:primaryType="nt:unstructured"
            imageRotate="0"/>
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/aside"
            cancelInheritParent="true"
            componentId="side">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <navlist
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/lists/navlist"
                    cancelInheritParent="true"/>
            </par>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <pagedetails
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"
                    accesskey="1">
                    <breadcrumb
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/breadcrumb"/>
                    <toolbar
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/navbar"
                        cancelInheritParent="true"/>
                </pagedetails>
                <text
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/content/text"
                    text="&lt;p>This section is made to showcase each component individualy.&lt;/p>&#xa;&lt;p>Each page in this section is dedicated to individual components so that they can be showcased and used for testing.&lt;/p>&#xa;"
                    textIsRich="true"/>
                <list
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/lists/list"
                    imageRotate="0"
                    listFrom="children"
                    listItemShowLink="false"
                    listLinkTitle="More Items"
                    listType="list"
                    parentPage="/content/aemdesign-showcase/au/en/component"
                    tagsMatch="any"/>
            </par>
        </article>
    </jcr:content>
    <base/>
</jcr:root>
