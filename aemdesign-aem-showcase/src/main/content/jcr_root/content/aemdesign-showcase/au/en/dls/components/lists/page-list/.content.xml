<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/dls"
        jcr:primaryType="cq:PageContent"
        jcr:title="Page List"
        sling:resourceType="aemdesign/components/template/dls">
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/aside">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container"/>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page_details
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"
                    componentId="page_details"
                    editable="{Boolean}false"
                    showBreadcrumb="false"
                    showPageDate="false"
                    showParsys="false"
                    showToolbar="false"
                    textIsRich="true"/>
                <contentblock_pagelist_basic
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_pagelist_basic">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_pagelist_basic_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_pagelist_basic_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Links&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <contentblock_pagelist_basic_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/pagelist"
                            componentId="contentblock_pagelist_basic_component"
                            limit="3"
                            listFrom="children"
                            listType="list"
                            parentPage="/content/aemdesign-showcase/au/en/dls/components/lists/pages"/>
                    </par>
                </contentblock_pagelist_basic>
                <contentblock_pagelist_images
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_pagelist_images">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_pagelist_images_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_pagelist_images_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Images&lt;/h2>&#xd;&#xa;&lt;p>List of clickable images.&lt;/p>&#xd;&#xa;"
                            textIsRich="true"/>
                        <contentblock_pagelist_images_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/pagelist"
                            componentId="contentblock_pagelist_images_component"
                            detailsBadge="image"
                            limit="3"
                            listFrom="children"
                            listType="list"
                            parentPage="/content/aemdesign-showcase/au/en/dls/components/lists/pages"/>
                    </par>
                </contentblock_pagelist_images>
                <contentblock_pagelist_cards_witda
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_pagelist_cards_witda">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_pagelist_cards_witda_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_pagelist_cards_witda_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Cards&lt;/h2>&#xd;&#xa;&lt;p>List of cards with an image, title, description and action.&lt;/p>&#xd;&#xa;"
                            textIsRich="true"/>
                        <contentblock_pagelist_cards_witda_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/pagelist"
                            badgeTitleType="h4"
                            componentId="contentblock_pagelist_cards_witda_component"
                            detailsBadge="cardImageTitleDescriptionAction"
                            limit="3"
                            listFrom="children"
                            listType="list"
                            parentPage="/content/aemdesign-showcase/au/en/dls/components/lists/pages"/>
                    </par>
                </contentblock_pagelist_cards_witda>
            </par>
        </article>
    </jcr:content>
</jcr:root>
