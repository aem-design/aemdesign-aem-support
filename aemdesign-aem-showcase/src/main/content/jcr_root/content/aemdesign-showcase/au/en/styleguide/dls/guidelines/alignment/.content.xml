<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
		  jcr:primaryType="cq:Page">
	<jcr:content
		cq:template="/conf/aemdesign-showcase/settings/wcm/templates/styleguide"
		jcr:primaryType="cq:PageContent"
		jcr:title="Alignment"
		sling:resourceType="aemdesign/components/template/styleguide">
		<aside
			jcr:primaryType="nt:unstructured"
			sling:resourceType="aemdesign/components/layout/aside">
			<par
				jcr:primaryType="nt:unstructured"
				sling:resourceType="aemdesign/components/layout/container"/>
		</aside>
		<article
			jcr:primaryType="nt:unstructured"
			sling:resourceType="aemdesign/components/layout/article">
			<par
				jcr:primaryType="nt:unstructured"
				sling:resourceType="aemdesign/components/layout/container">
				<page_details
					jcr:mixinTypes="[cq:Taggable]"
					jcr:primaryType="nt:unstructured"
					sling:resourceType="aemdesign/components/details/page-details"
					componentId="page_details"
					componentModifiers="[aemdesign-showcase:component-style-modifier/background/primary/bg-yellow-pattern]"
					componentTheme="[aemdesign-showcase:component-style-theme/styleguide/page-details]"
					editable="{Boolean}false"
					showBreadcrumb="false"
					showPageDate="false"
					showParsys="false"
					showToolbar="false"
					textIsRich="true">
					<secondaryImage
						jcr:primaryType="nt:unstructured"
						sling:resourceType="aemdesign/components/media/image"/>
					<thumbnail
						jcr:primaryType="nt:unstructured"
						sling:resourceType="aemdesign/components/media/image"/>
					<bgimage
						jcr:primaryType="nt:unstructured"
						sling:resourceType="aemdesign/components/media/image"/>
					<ogImage
						jcr:primaryType="nt:unstructured"
						sling:resourceType="aemdesign/components/media/image"/>
				</page_details>
				<contentblock_single_link_text_align
					jcr:primaryType="nt:unstructured"
					sling:resourceType="aemdesign/components/layout/contentblock"
					componentId="contentblock_single_link_text_align"
					linkPagesRight="/content/aemdesign"
					linksRightTitle="Link">
					<par
						jcr:primaryType="nt:unstructured"
						sling:resourceType="aemdesign/components/layout/container">
						<text_single_link_text_align_title
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/content/text"
							componentId="text_single_link_text_align_title"
							text="&lt;h4>Single Links:&amp;nbsp;Text alignment&lt;/h4>&#xd;&#xa;"
							textIsRich="true"/>
						<text_single_link_text_align_left_heading
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/content/text"
							componentId="text_single_link_text_align_left_heading"
							text="&lt;h5>Left aligned: Text &amp;gt; Alignment &amp;gt; Left&lt;/h5>&#xa;"
							textIsRich="true"/>
						<contentblock_single_link_text_align_left
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/layout/contentblock"
							componentId="contentblock_single_link_text_align_left"
							componentModifiers="[aemdesign-showcase:component-style-modifier/text/alignment/left]"
							variant="parsys">
							<par
								jcr:primaryType="nt:unstructured"
								sling:resourceType="aemdesign/components/layout/container">
								<link_single_link_text_align_left
									jcr:primaryType="nt:unstructured"
									sling:resourceType="aemdesign/components/content/link"
									componentId="link_single_link_text_align_left"
									componentModifiers="[aemdesign-showcase:component-style-modifier/button/secondary-charcoal]"
									label="Link"
									variant="button"/>
							</par>
							<bgimage
								jcr:primaryType="nt:unstructured"
								sling:resourceType="aemdesign/components/media/image"/>
							<bgvideo
								jcr:primaryType="nt:unstructured"
								sling:resourceType="aemdesign/components/media/video"/>
						</contentblock_single_link_text_align_left>
						<text_single_link_text_align_center_heading
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/content/text"
							componentId="text_single_link_text_align_center_heading"
							text="&lt;h5>Centre aligned: Text &amp;gt; Alignment &amp;gt; Center&lt;/h5>&#xd;&#xa;"
							textIsRich="true"/>
						<contentblock_single_link_text_align_center
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/layout/contentblock"
							componentId="contentblock_single_link_text_align_center"
							componentModifiers="[aemdesign-showcase:component-style-modifier/text/alignment/center]"
							variant="parsys">
							<par
								jcr:primaryType="nt:unstructured"
								sling:resourceType="aemdesign/components/layout/container">
								<link_single_link_text_align_center
									jcr:primaryType="nt:unstructured"
									sling:resourceType="aemdesign/components/content/link"
									componentId="link_single_link_text_align_center"
									componentModifiers="[aemdesign-showcase:component-style-modifier/button/secondary-charcoal]"
									label="Link"
									variant="button"/>
							</par>
							<bgimage
								jcr:primaryType="nt:unstructured"
								sling:resourceType="aemdesign/components/media/image"/>
							<bgvideo
								jcr:primaryType="nt:unstructured"
								sling:resourceType="aemdesign/components/media/video"/>
						</contentblock_single_link_text_align_center>
						<text_single_link_text_align_right_heading
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/content/text"
							componentId="text_single_link_text_align_right_heading"
							text="&lt;h5>Right aligned:&amp;nbsp;Text &amp;gt; Alignment &amp;gt; Right&lt;/h5>&#xd;&#xa;"
							textIsRich="true"/>
						<contentblock_single_link_text_align_right
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/layout/contentblock"
							componentId="contentblock_single_link_text_align_right"
							componentModifiers="[aemdesign-showcase:component-style-modifier/text/alignment/right]"
							variant="parsys">
							<par
								jcr:primaryType="nt:unstructured"
								sling:resourceType="aemdesign/components/layout/container">
								<link_single_link_text_align_right
									jcr:primaryType="nt:unstructured"
									sling:resourceType="aemdesign/components/content/link"
									componentId="link_single_link_text_align_right"
									componentModifiers="[aemdesign-showcase:component-style-modifier/button/secondary-charcoal]"
									label="Link"
									variant="button"/>
							</par>
							<bgimage
								jcr:primaryType="nt:unstructured"
								sling:resourceType="aemdesign/components/media/image"/>
							<bgvideo
								jcr:primaryType="nt:unstructured"
								sling:resourceType="aemdesign/components/media/video"/>
						</contentblock_single_link_text_align_right>
					</par>
					<bgimage
						jcr:primaryType="nt:unstructured"
						sling:resourceType="aemdesign/components/media/image"/>
					<bgvideo
						jcr:primaryType="nt:unstructured"
						sling:resourceType="aemdesign/components/media/video"/>
				</contentblock_single_link_text_align>
			</par>
		</article>
	</jcr:content>
</jcr:root>
