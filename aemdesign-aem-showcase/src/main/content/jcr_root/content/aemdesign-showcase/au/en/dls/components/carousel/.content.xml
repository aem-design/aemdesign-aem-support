<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
		  xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
		  jcr:primaryType="cq:Page">
	<jcr:content
		cq:template="/conf/aemdesign-showcase/settings/wcm/templates/dls"
		jcr:primaryType="cq:PageContent"
		jcr:title="Carousel"
		sling:resourceType="aemdesign/components/template/dls">
		<aside
			jcr:primaryType="nt:unstructured"
			sling:resourceType="aemdesign/components/layout/aside"
			componentId="aside">
			<par
				jcr:primaryType="nt:unstructured"
				sling:resourceType="aemdesign/components/layout/container"/>
		</aside>
		<article
			jcr:primaryType="nt:unstructured"
			sling:resourceType="aemdesign/components/layout/article"
			componentId="article">
			<par
				jcr:primaryType="nt:unstructured"
				sling:resourceType="aemdesign/components/layout/container">
				<page_details
					jcr:mixinTypes="[cq:Taggable]"
					jcr:primaryType="nt:unstructured"
					sling:resourceType="aemdesign/components/details/page-details"
					componentId="page_details"
					showBreadcrumb="false"
					showPageDate="false"
					showToolbar="false"
					textIsRich="true"/>
                <contentblock_carousel_basic
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_carousel_basic">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_carousel_basic_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_carousel_basic_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Basic Carousel&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <contentblock_carousel_basic_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/pagelist"
                            componentId="contentblock_carousel_basic_component"
                            componentModules="[aemdesign-showcase:component-style-module/lists/carousel]"
                            detailsBadge="cardImageTagTitleAction"
                            listFrom="children"
                            parentPage="/content/aemdesign-showcase/au/en/component/lists/page-list/cards"/>
                    </par>
                </contentblock_carousel_basic>
			</par>
		</article>
	</jcr:content>
</jcr:root>
