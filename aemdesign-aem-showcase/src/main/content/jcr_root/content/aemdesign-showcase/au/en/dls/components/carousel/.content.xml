							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/content/text"
							componentId="contentblock_carousel_pagelist_default_title"
							text="&lt;h4>Basic Carousel - Default Background (Page List)&lt;/h4>&#xd;&#xa;"
							textIsRich="true"/>
						<contentblock_carousel_pagelist_default_list
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/lists/pagelist"
							badgeLinkStyle="[aemdesign-showcase:component-style-modifier/button/link]"
							componentId="contentblock_carousel_pagelist_default_list"
							componentModules="[aemdesign-showcase:component-style-module/lists/carousel]"
							detailsBadge="cardImageTagTitleAction"
							listFrom="children"
							parentPage="/content/aemdesign-showcase/au/en/component/lists/page-list/cards"
							sortOrder="asc"
							tagsMatch="any"/>
					</par>
				</contentblock_carousel_pagelist_default>
				<contentblock_carousel_pagelist_grey
					jcr:primaryType="nt:unstructured"
