					<par
						jcr:primaryType="nt:unstructured"
						sling:resourceType="aemdesign/components/layout/container">
						<contentblock_carousel_pagelist_default_short_title
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/content/text"
							badgeLinkStyle="[aemdesign-showcase:component-style-modifier/button/link]"
							componentId="contentblock_carousel_pagelist_default_short_title"
							text="&lt;h4>Dynamic Carousel (Page List)&lt;/h4>&lt;p>This carousel won't take affect until the screen real estate becomes too small for each item to fit.&lt;/p>&#xd;&#xa;"
							textIsRich="true"/>
						<contentblock_carousel_pagelist_default_short_list
							jcr:primaryType="nt:unstructured"
							sling:resourceType="aemdesign/components/lists/pagelist"
