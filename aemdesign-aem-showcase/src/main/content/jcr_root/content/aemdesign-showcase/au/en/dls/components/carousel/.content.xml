							sling:resourceType="aemdesign/components/content/text"
							badgeLinkStyle="[aemdesign-showcase:component-style-modifier/button/link]"
							componentId="contentblock_carousel_pagelist_default_split_list_title"
							text="&lt;h4>Dynamic Carousel (Page List)&lt;/h4>&#xd;&#xa;&lt;p>This carousel won't take affect until a user is on a mobile/tablet device, on desktop they will see the Page List as normal and stacked.&lt;/p>&#xd;&#xa;"
