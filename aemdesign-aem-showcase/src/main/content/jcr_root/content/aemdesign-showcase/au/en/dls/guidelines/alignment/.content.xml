<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/dls"
        jcr:primaryType="cq:PageContent"
        jcr:title="Alignment"
        sling:resourceType="aemdesign/components/template/dls">
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/aside">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container"/>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page_details
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"
                    componentId="page_details"
                    editable="{Boolean}false"
                    showBreadcrumb="false"
                    showPageDate="false"
                    showParsys="false"
                    showToolbar="false"
                    textIsRich="true"/>
                <contentblock_left_text_align
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_left_text_align">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_left_text_align_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_left_text_align_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Left Alignment (default)&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <contentblock_left_text_align_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/contentblock"
                            componentId="contentblock_left_text_align_component"
                            componentModifiers="[aemdesign-showcase:component-style-modifier/text/alignment/left]">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="aemdesign/components/layout/container">
                                <contentblock_left_text_align_component_text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/text"
                                    componentId="contentblock_left_text_align_component_text"
                                    text="&lt;p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.&lt;/p>&#xd;&#xa;"
                                    textIsRich="true"/>
                            </par>
                        </contentblock_left_text_align_component>
                    </par>
                </contentblock_left_text_align>
                <contentblock_center_text_align
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_center_text_align">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_center_text_align_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_center_text_align_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Center Alignment&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <contentblock_center_text_align_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/contentblock"
                            componentId="contentblock_center_text_align_component"
                            componentModifiers="[aemdesign-showcase:component-style-modifier/text/alignment/center]">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="aemdesign/components/layout/container">
                                <contentblock_center_text_align_component_text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/text"
                                    componentId="contentblock_center_text_align_component_text"
                                    text="&lt;p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.&lt;/p>&#xd;&#xa;"
                                    textIsRich="true"/>
                            </par>
                        </contentblock_center_text_align_component>
                    </par>
                </contentblock_center_text_align>
                <contentblock_right_text_align
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_right_text_align">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_right_text_align_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_right_text_align_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Right Alignment&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <contentblock_right_text_align_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/contentblock"
                            componentId="contentblock_right_text_align_component"
                            componentModifiers="[aemdesign-showcase:component-style-modifier/text/alignment/right]">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="aemdesign/components/layout/container">
                                <contentblock_right_text_align_component_text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/content/text"
                                    componentId="contentblock_right_text_align_component_text"
                                    text="&lt;p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.&lt;/p>&#xd;&#xa;"
                                    textIsRich="true"/>
                            </par>
                        </contentblock_right_text_align_component>
                    </par>
                </contentblock_right_text_align>
            </par>
        </article>
    </jcr:content>
</jcr:root>
