<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[aemdesign-showcase:content-type/page]"
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/twocolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="List"
        sling:resourceType="aemdesign/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article"
            componentId="article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"
                    componentId="page-details"/>
                <contentblock1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock1"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="List: Default variant using Default badge"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <list
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/list"
                            componentId="list1"
                            listFrom="children"
                            listType="list"
                            parentPage="/content/aemdesign-showcase/au/en/component/lists/list/images"/>
                    </par>
                </contentblock1>
                <contentblock2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock2"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="List: Default variant using Icon badge"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <list
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/list"
                            componentId="list2"
                            detailsBadge="icon"
                            listFrom="children"
                            listType="list"
                            parentPage="/content/aemdesign-showcase/au/en/component/lists/list/icons"/>
                    </par>
                </contentblock2>
                <contentblock3
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock3"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="List: Default variant using Image badge"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <list
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/list"
                            componentId="list3"
                            detailsBadge="image"
                            listFrom="children"
                            listType="list"
                            parentPage="/content/aemdesign-showcase/au/en/component/lists/list/images"/>
                    </par>
                </contentblock3>
                <contentblock4
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock4"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="List: Default variant using Card badge"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <list
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/list"
                            componentId="list4"
                            detailsBadge="cardImageTitleCategoryDescription"
                            listFrom="children"
                            listType="list"
                            parentPage="/content/aemdesign-showcase/au/en/component/lists/list/images"/>
                    </par>
                </contentblock4>
                <contentblock5
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock5"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="List: Default variant using Card Icon badge"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <list
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/list"
                            componentId="list5"
                            detailsBadge="cardIcon"
                            listFrom="children"
                            listType="list"
                            parentPage="/content/aemdesign-showcase/au/en/content/icons"/>
                    </par>
                </contentblock5>
                <contentblock6
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock6"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="List: Default Empty"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <list
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/list"
                            componentId="list6"/>
                    </par>
                </contentblock6>
                <contentblock7
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock7"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="List: Pages with no Details"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <list
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/list"
                            componentId="list7"
                            listFrom="children"
                            parentPage="/content/aemdesign-showcase/au/en/component/lists/list/nodetails"/>
                    </par>
                </contentblock7>
                <contentblock8
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock8"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="List: Pages search with By Tags"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <list
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/list"
                            componentId="list8"
                            listFrom="tags"
                            sortOrder="asc"
                            tags="[aemdesign-showcase:content-type/page,aemdesign-showcase:content-type/page/contact]"
                            tagsMatch="any"
                            tagsSearchRoot="/content/aemdesign-showcase/au/en/component/lists/list/tags">
                        </list>
                    </par>
                </contentblock8>
                <contentblock9
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock8"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="List: Pages search with By Tags with Tags not Specified"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <list
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/list"
                            componentId="list9"
                            listFrom="tags"
                            sortOrder="asc"
                            tagsMatch="any"
                            tagsSearchRoot="/content/aemdesign-showcase/au/en/component/lists/list/tags">
                        </list>
                    </par>
                </contentblock9>
            </par>
        </article>
    </jcr:content>
    <images/>
    <icons/>
    <nodetails/>
    <tags/>
</jcr:root>
