<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/dls"
        jcr:primaryType="cq:PageContent"
        jcr:title="News List"
        sling:resourceType="aemdesign/components/template/dls">
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/aside">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container"/>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page_details
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"
                    componentId="page_details"
                    editable="{Boolean}false"
                    showBreadcrumb="false"
                    showPageDate="false"
                    showParsys="false"
                    showToolbar="false"
                    textIsRich="true"/>
                <contentblock
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_1872776108
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/contentblock">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="aemdesign/components/layout/container">
                                <newslist
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/lists/newslist"
                                    componentId="newslist-1"
                                    detailsBadge="cardImageTitleCategoryActionDate"
                                    listFrom="static"
                                    pages="[/content/aemdesign-showcase/au/en/styleguide/components/news-details,/content/aemdesign-showcase/au/en/styleguide/components/news-details,/content/aemdesign-showcase/au/en/styleguide/components/news-details]"
                                    tagsMatch="any">
                                    <badgeThumbnail
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="aemdesign/components/media/image"/>
                                </newslist>
                            </par>
                        </contentblock_1872776108>
                    </par>
                </contentblock>
            </par>
        </article>
    </jcr:content>
</jcr:root>
