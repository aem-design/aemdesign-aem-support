<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/styleguide"
        jcr:primaryType="cq:PageContent"
        jcr:title="Typography"
        sling:resourceType="aemdesign/components/template/styleguide">
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/aside">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container"/>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page_details
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"
                    hideDescription="true"
                    hideTitle="false"
                    showBreadcrumb="false"
                    showPageDate="false"
                    showParsys="true"
                    showToolbar="false"
                    textIsRich="true"/>
                <typography_standard
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="typography_standard">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <typography_standard_text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="typography_standard_text"
                            text="&lt;h1>Heading 1&lt;/h1>&#xd;&#xa;&lt;h2>Heading 2&lt;/h2>&#xd;&#xa;&lt;h3>Heading 3&lt;/h3>&#xd;&#xa;&lt;h4>Heading 4&lt;/h4>&#xd;&#xa;&lt;h5>Heading 5&lt;/h5>&lt;p>&lt;span class=&quot;text-cap-underline&quot;>&lt;span class=&quot;text--cap-underline&quot;>body default with text cap underline: We belieave advertising is an integral part of new media today, to the point where people rarely stop to hink about it. That's why we offer Advertising at aemdesign.&lt;/span>&lt;/span>&lt;br />&#xd;&#xa;&lt;/p>&#xd;&#xa;&lt;p>&lt;b>body bold : We belieave advertising is an integral part of new media today, to the point where people rarely stop to hink about it. That's why we offer Advertising at aemdesign.&lt;/b>&lt;/p>&#xd;&#xa;&lt;p>&lt;small>body small : We belieave advertising is an integral part of new media today, to the point where people rarely stop to hink about it. That's why we offer Advertising at aemdesign.&lt;/small>&lt;/p>&#xd;&#xa;&lt;p>&lt;small>&lt;span class=&quot;fw-600&quot;>body small bold : We belieave advertising is an integral part of new media today, to the point where people rarely stop to hink about it. That's why we offer Advertising at aemdesign.&lt;/span>&lt;/small>&lt;/p>&#xd;&#xa;&lt;p>&lt;a href=&quot;http://www.google.fr&quot; target=&quot;_blank&quot;>Link: his is a text link&lt;/a>&lt;/p>&#xd;&#xa;&lt;p>&lt;i>Italic: This text has added emphasis&lt;/i>&lt;/p>&#xd;&#xa;&lt;p>This text is deleted&lt;/p>&#xd;&#xa;&lt;ul>&#xd;&#xa;&lt;li>Simple style list items&lt;/li>&#xd;&#xa;&lt;li>Simple style list items  &lt;/li>&#xd;&#xa;&lt;/ul>&#xd;&#xa;&lt;ol>&#xd;&#xa;&lt;li>Ordered style list items&lt;/li>&#xd;&#xa;&lt;li>Ordered style list items&lt;/li>&#xd;&#xa;&lt;/ol>&#xd;&#xa;"
                            textIsRich="true"/>
                    </par>
                </typography_standard>
            </par>
        </article>
    </jcr:content>
</jcr:root>
