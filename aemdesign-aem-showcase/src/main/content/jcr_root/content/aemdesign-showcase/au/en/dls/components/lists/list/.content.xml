<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/dls"
        jcr:primaryType="cq:PageContent"
        jcr:title="List"
        sling:resourceType="aemdesign/components/template/dls">
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/aside">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container"/>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page_details
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"
                    componentId="page_details"
                    description="Generic list of content which can ingest any details components. This component is useful for displaying multiple different types of pages in a way that keeps their output generic."
                    editable="{Boolean}false"
                    showBreadcrumb="false"
                    showPageDate="false"
                    showParsys="false"
                    showToolbar="false"
                    textIsRich="true"/>
                <contentblock_list_cards_custom
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_list_cards_custom">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_list_cards_custom_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_list_cards_custom_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Cards (using Generic Details)&lt;/h2>&#xd;&#xa;&lt;p>List of cards with a title and description. These are built using the &lt;strong>card&lt;/strong> field template&lt;/p>&#xd;&#xa;"
                            textIsRich="true"/>
                        <contentblock_list_cards_custom_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/list"
                            badgeCustom="true"
                            badgeFields="[title,description]"
                            badgeFieldsTemplate="card"
                            badgeTitleType="h4"
                            componentId="contentblock_list_cards_custom_component"
                            limit="3"
                            listFrom="children"
                            listType="list"
                            parentPage="/content/aemdesign-showcase/au/en/dls/components/lists/support/generic-details"/>
                    </par>
                </contentblock_list_cards_custom>
                <contentblock_list_cards_custom_wi
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_list_cards_custom_wi">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_list_cards_custom_wi_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_list_cards_custom_wi_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Cards (using Generic Details)&lt;/h2>&#xd;&#xa;&lt;p>List of cards with an image and description. These are built using the &lt;strong>card-image&lt;/strong> field template&lt;/p>&#xd;&#xa;"
                            textIsRich="true"/>
                        <contentblock_list_cards_custom_wi_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/list"
                            badgeCustom="true"
                            badgeFields="[description]"
                            badgeFieldsTemplate="card-image"
                            badgeTitleType="h4"
                            componentId="contentblock_list_cards_custom_wi_component"
                            limit="3"
                            listFrom="children"
                            listType="list"
                            parentPage="/content/aemdesign-showcase/au/en/dls/components/lists/support/generic-details"/>
                    </par>
                </contentblock_list_cards_custom_wi>
                <contentblock_list_cards_witda
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_list_cards_witda">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_list_cards_witda_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_list_cards_witda_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Cards (using Page Details)&lt;/h2>&#xd;&#xa;&lt;p>List of cards with an image, title, description and action.&lt;/p>&#xd;&#xa;"
                            textIsRich="true"/>
                        <contentblock_list_cards_witda_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/lists/list"
                            badgeTitleType="h4"
                            componentId="contentblock_list_cards_witda_component"
                            detailsBadge="cardImageTitleDescriptionAction"
                            limit="3"
                            listFrom="children"
                            listType="list"
                            parentPage="/content/aemdesign-showcase/au/en/dls/components/lists/support/page-details"/>
                    </par>
                </contentblock_list_cards_witda>
            </par>
        </article>
    </jcr:content>
</jcr:root>
