<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[aemdesign-showcase:content-type/page]"
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/twocolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="External"
        sling:resourceType="aemdesign/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"/>
                <contentblock1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock1"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="External: iFrame variant with Parameters"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <external
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/external"
                            componentId="external1"
                            height="900"
                            showScrollbar="false"
                            target="/content/aemdesign-showcase/au/en/component/content/external/page1.html?wcmmode=disabled"
                            variant="iframe"
                            width="600"/>
                    </par>
                </contentblock1>
                <contentblock2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock2"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="External: iFrame variant without Parameters"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <external
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/external"
                            componentId="external2"
                            target="/content/aemdesign-showcase/au/en/component/content/external/page1.html?wcmmode=disabled"
                            variant="iframe"/>
                    </par>
                </contentblock2>
                <contentblock3
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock3"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="External: SSI Include variant"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <external
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/external"
                            componentId="external3"
                            target="http://google.com"
                            variant="include"/>
                    </par>
                </contentblock3>
                <contentblock4
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock4"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="External: Server Import variant"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <external
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/external"
                            componentId="external4"
                            target="file:////aem/crx-quickstart/eula-en_US.html"
                            variant="import"/>
                    </par>
                </contentblock4>
            </par>
        </article>
    </jcr:content>
    <page1/>
</jcr:root>
