<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/dls"
        jcr:primaryType="cq:PageContent"
        jcr:title="Breadcrumbs"
        sling:resourceType="aemdesign/components/template/dls">
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/aside">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container"/>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <page_details
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"
                    description="&lt;p>Breadcrumbs enable users to quickly see their current context/location within the content tree.&lt;/p>&#xd;&#xa;"
                    editable="{Boolean}false"
                    showBreadcrumb="false"
                    showPageDate="false"
                    showParsys="false"
                    showToolbar="false"
                    textIsRich="true"/>
                <contentblock_breadcrumb_standard
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_breadcrumb_standard">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_breadcrumb_standard_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_breadcrumb_standard_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Standard&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <contentblock_breadcrumb_standard_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/breadcrumb"
                            componentId="contentblock_breadcrumb_standard_component"/>
                    </par>
                </contentblock_breadcrumb_standard>
                <contentblock_breadcrumb_start_level
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_breadcrumb_start_level">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_breadcrumb_start_level_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_breadcrumb_start_level_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Start at Level 3&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <contentblock_breadcrumb_start_level_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/breadcrumb"
                            componentId="contentblock_breadcrumb_start_level_component"
                            startLevel="3"/>
                    </par>
                </contentblock_breadcrumb_start_level>
                <contentblock_breadcrumb_end_level
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/layout/contentblock"
                    componentId="contentblock_breadcrumb_end_level">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <contentblock_breadcrumb_end_level_heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/content/text"
                            componentId="contentblock_breadcrumb_end_level_heading"
                            text="&lt;h2 class=&quot;h5 strong&quot;>Start at Level 3 and end at Level 5&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <contentblock_breadcrumb_end_level_component
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/layout/breadcrumb"
                            componentId="contentblock_breadcrumb_end_level_component"
                            endLevel="5"
                            startLevel="3"/>
                    </par>
                </contentblock_breadcrumb_end_level>
            </par>
        </article>
    </jcr:content>
</jcr:root>
