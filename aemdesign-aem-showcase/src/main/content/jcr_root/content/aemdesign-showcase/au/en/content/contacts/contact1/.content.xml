<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[aemdesign-showcase:content-type/page]"
        cq:template="/conf/aemdesign-showcase/settings/wcm/templates/twocolumn"
        jcr:description="Contact1 Description"
        jcr:primaryType="cq:PageContent"
        jcr:title="Contact1"
        sling:resourceType="aemdesign/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <contactdetails
                    cq:tags="[aemdesign-showcase:content-type/widgets]"
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/contact-details"
                    componentId="contact-details"
                    description="Custom Description"
                    descriptionFormat="aemdesign-showcase:component-style-theme/details/contact-details/format/description/jobTitleAtEmployerWithEmail"
                    email="max.barrass@gmail.com"
                    employer="AEM.Design"
                    familyName="Barrass"
                    givenName="Max"
                    honorificPrefix="aemdesign-showcase:component-dialog/honorific/prefix/mr"
                    jobTitle="Founder"
                    showBreadcrumb="false"
                    showParsys="false"
                    showToolbar="false"/>
            </par>
        </article>
    </jcr:content>
</jcr:root>
