<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:mixinTypes="[rep:AccessControllable]"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:allowedTemplates="[/conf/aemdesign/settings/wcm/templates/.*]"
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:designPath="/apps/settings/wcm/designs/aemdesign"
        cq:template="/conf/aemdesign/settings/wcm/templates/twocolumn"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="AEM.Design"
        sling:resourceType="aemdesign/components/template/base"
        redirectTarget="/content/aemdesign-showcase/en">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="/apps/aemdesign/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemdesign/components/layout/container">
                <pagedetails
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemdesign/components/details/page-details"/>
            </par>
        </article>
    </jcr:content>
</jcr:root>
